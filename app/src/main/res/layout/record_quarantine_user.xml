<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wavetop"
    tools:context=".Application.Vaccination.User.QuarantineRecord">

    <TextView
        android:id="@+id/title1"
        android:layout_width="303dp"
        android:layout_height="114dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="56dp"
        android:text="Quarantine Record"
        android:textColor="#fff"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:layout_width="310dp"
        android:layout_height="498dp"
        android:layout_row="0"
        android:layout_rowWeight="1"
        android:layout_column="0"
        android:layout_columnWeight="1"
        android:layout_gravity="fill"
        android:layout_margin="8dp"
        app:cardBackgroundColor="#3E6DB2"
        app:cardCornerRadius="8dp"
        app:cardElevation="8dp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="198dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:layout_width="277dp"
                android:layout_height="63dp"
                tools:layout_editor_absoluteX="11dp"
                tools:layout_editor_absoluteY="239dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="264dp"
                    android:layout_height="51dp">

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="114dp"
                        android:layout_height="25dp"
                        android:text="Health Status"
                        tools:layout_editor_absoluteX="16dp"
                        tools:layout_editor_absoluteY="16dp" />

                    <Button
                        android:id="@+id/gobutton4"
                        android:layout_width="90dp"
                        android:layout_height="38dp"
                        android:text="GO"
                        tools:layout_editor_absoluteX="172dp"
                        tools:layout_editor_absoluteY="14dp" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:layout_editor_absoluteX="3dp">

                <androidx.cardview.widget.CardView
                    android:layout_width="280dp"
                    android:layout_height="66dp"
                    tools:layout_editor_absoluteX="9dp"
                    tools:layout_editor_absoluteY="147dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Update Quarantine Record"
                            tools:layout_editor_absoluteX="6dp"
                            tools:layout_editor_absoluteY="24dp" />

                        <Button
                            android:id="@+id/go_update"
                            android:layout_width="81dp"
                            android:layout_height="38dp"
                            android:text="GO"
                            tools:layout_editor_absoluteX="183dp"
                            tools:layout_editor_absoluteY="16dp" />186dp"
                        tools:layout_editor_absoluteY="9dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="141dp"
                    android:layout_height="157dp"
                    app:srcCompat="@drawable/img_2"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="1dp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp">

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="172dp"
                        android:layout_height="71dp"
                        android:text="Name:Aina Syakirah
                                      User ID: +6012543890
                                      Covid19 Status: Positive"
                        android:textStyle="bold"
                        tools:layout_editor_absoluteX="142dp"
                        tools:layout_editor_absoluteY="42dp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="354dp"
                        tools:layout_editor_absoluteX="0dp"
                        tools:layout_editor_absoluteY="114dp">

                        <androidx.cardview.widget.CardView
                            android:layout_width="278dp"
                            android:layout_height="61dp"
                            tools:layout_editor_absoluteX="11dp"
                            tools:layout_editor_absoluteY="210dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/textView13"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Medical Status"
                                    tools:layout_editor_absoluteX="16dp"
                                    tools:layout_editor_absoluteY="21dp" />

                                <Button
                                    android:id="@+id/gobutton5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="GO"
                                    tools:layout_editor_absoluteX="174dp"
                                    tools:layout_editor_absoluteY="6dp" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="274dp"
                            android:layout_height="51dp"
                            tools:layout_editor_absoluteX="14dp"
                            tools:layout_editor_absoluteY="296dp" >

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/textView14"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Close Contact Details"
                                    tools:layout_editor_absoluteX="14dp"
                                    tools:layout_editor_absoluteY="14dp" />

                                <Button
                                    android:id="@+id/gobutton6"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="GO"
                                    tools:layout_editor_absoluteX="170dp"
                                    tools:layout_editor_absoluteY="1dp" />
                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>