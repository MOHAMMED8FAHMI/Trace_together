<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wavetop"
    tools:context=".Application.Vaccination.moh.homepage">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="46dp"
        android:layout_height="54dp"
        app:srcCompat="@drawable/logout"
        tools:layout_editor_absoluteX="349dp"
        tools:layout_editor_absoluteY="26dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="174dp"
        android:layout_height="74dp"
        android:text="MOH"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="35dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="245dp"
        android:layout_height="71dp"
        android:text="User Quarantine Record"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="#fff"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="65dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="132dp"
        android:layout_height="38dp"
        android:text="Insert User ID:"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="255dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="248dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="313dp"
        android:layout_height="76dp"
        app:cardBackgroundColor="#E7E7E7"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="354dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="90dp"
                android:layout_height="71dp"
                app:srcCompat="@drawable/check"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="3dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="View Covid19 status"
                tools:layout_editor_absoluteX="129dp"
                tools:layout_editor_absoluteY="25dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="313dp"
        android:layout_height="76dp"
        app:cardBackgroundColor="#E7E7E7"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="457dp" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="90dp"
                android:layout_height="66dp"
                app:srcCompat="@drawable/update"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="5dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Update Quarantine Record"
                tools:layout_editor_absoluteX="118dp"
                tools:layout_editor_absoluteY="29dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="313dp"
        android:layout_height="76dp"
        app:cardBackgroundColor="#E7E7E7"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="568dp" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="85dp"
                android:layout_height="71dp"
                app:srcCompat="@drawable/img"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="3dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="View Self Test Result"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="29dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>